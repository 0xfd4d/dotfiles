Xft.autohint: 0
Xft.dpi: 72
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

urxvt*title: terminal

urxvt*font: xft:Droid Sans Mono:size=13
! urxvt*boldFont:

urxvt*perl-ext-common: default,clipboard,url-select,font-size

urxvt*keysym.M-u: perl:url-select:select_next
urxvt*url-select.launcher: xdg-open

urxvt*keysym.M-c: perl:clipboard:copy
urxvt*keysym.M-v: perl:clipboard:paste
urxvt*keysym.M-C-v: perl:clipboard:paste_escaped

urxvt*keysym.C-Up: font-size:increase
urxvt*keysym.C-Down: font-size:decrease

urxvt*scrollBar: false

urxvt*saveLines: 99999

urxvt*scrollTtyOutput: false
urxvt*scrollWithBuffer: true
urxvt*scrollTtyKeypress: true

urxvt*cursorBlink: false
urxvt*cursorUnderline: false

urxvt*background:       #000000
urxvt*foreground:       #cccccc
urxvt*color8:           #656565
urxvt*color0:           #000000
urxvt*color1:           #e85555
urxvt*color9:           #e85555
urxvt*color2:           #88ab77
urxvt*color10:          #8ccd6c
urxvt*color3:           #d0d9ad
urxvt*color11:          #c5d67d
urxvt*color4:           #2d3753
urxvt*color12:          #2D3753
urxvt*color5:           #cdb4c9
urxvt*color13:          #cdb4c9
urxvt*color6:           #505b6c
urxvt*color14:          #505b6c
urxvt*color7:           #cccccc
urxvt*color15:          #cccccc


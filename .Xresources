Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.dpi: 96
Xft.rgba: rgb

Xcursor.theme: default

urxvt.title: terminal

! urxvt.font: -*-fixed-medium-r-*-*-12-*-*-*-*-*-iso10646-*
urxvt.font: -*-fixed-medium-r-*-*-13-*-*-*-*-*-iso10646-*
! urxvt.boldFont:
! urxvt.boldItalicFont:

urxvt.perl-ext-common: default,clipboard,url-select,keyboard-select

urxvt.keysym.M-u: perl:url-select:select_next
urxvt.url-select.launcher: xdg-open

urxvt.keysym.M-d: perl:keyboard-select:activate

! urxvt.keysym.M-c: perl:clipboard:copy
! urxvt.keysym.M-v: perl:clipboard:paste
! urxvt.keysym.M-C-v: perl:clipboard:paste_escaped

urxvt.saveLines: 1024

urxvt.internalBorder: 0
urxvt.externalBorder: 0

urxvt.scrollBar: false

urxvt.scrollstyle: plain
urxvt.scrollTtyOutput: false
urxvt.scrollWithBuffer: true
urxvt.scrollTtyKeypress: true

urxvt.cursorBlink: false
urxvt.cursorUnderline: false

#include ".Xresources.d/hybrid"

! urxvt.transparent: true
! urxvt.shading: 20

! urxvt*depth: 32
! urxvt*background: rgba:0000/0000/0000/7f00
!
